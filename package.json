{
  "name": "electron-app",
  "productName": "electron-app",
  "version": "1.0.0",
  "description": "Basic electron application with openlayers map",
  "main": "./.webpack/main",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "rebuild-sqlite3": "electron-rebuild -f -w better-sqlite3",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/migrations/electronApp",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate ./src/migrations/electronApp -d ./src/database/connection.ts",
    "migration:up": "typeorm-ts-node-commonjs migration:run -d ./src/database/connection.ts",
    "migration:down": "typeorm-ts-node-commonjs migration:revert -d ./src/database/connection.ts"
  },
  "keywords": [],
  "author": {
    "name": "Rohit",
    "email": "rohit.bhandari@triona.fi"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@electron-forge/plugin-webpack": "^6.0.5",
    "@electron-forge/publisher-github": "^6.0.5",
    "@types/better-sqlite3": "^7.6.3",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "electron": "23.1.2",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@mui/styled-engine-sc": "^5.11.11",
    "@mui/styles": "^5.11.12",
    "@mui/system": "^5.11.12",
    "@reduxjs/toolkit": "^1.9.3",
    "better-sqlite3": "^8.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "ol": "^7.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "redux": "^4.2.1",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.3.8",
    "typeorm": "^0.3.12",
    "update-electron-app": "^2.0.1",
    "uuid": "^8.3.2"
  }
}
